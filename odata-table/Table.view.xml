<mvc:View 
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	controllerName="sap.ui.jerry.odatatable.table">
	<Text text="This is table title"/>
	<Table id="producttable" items="{/Products}">
	<columns>
		<Column width="12em">
			<Text text="ProductID" />
		</Column>
		
		<Column demandPopin="true">
			<Text text="ProductName" />
		</Column>	
	</columns>
	
	<items>
		<ColumnListItem>
			<cells>
				<Text text="{ProductID}"/>
				<Text text="{ProductName}"/>
			</cells>
		</ColumnListItem>
	</items>
	</Table>
</mvc:View>